# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'AnimalCherish_iOS' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for AppName
  pod 'Alamofire', '~> 5.0'
  pod 'SwiftyJSON', '~> 4.0'
  pod 'Charts', '~> 3.3.0'
  pod 'SwiftFormat/CLI'

  target 'AnimalCherish_iOSTests' do
    inherit! :search_paths
     pod 'Alamofire', '~> 5.0'
  end

end
